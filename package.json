{
  "name": "studio-2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "bump:safe": "npx npm-check-updates --reject \"recharts, vite, zod\" -u",
    "bump:tvads": "npx npm-check-updates -u \"@upscale-ai/control-room\"",
    "check:verbose": "npm run typecheck:verbose && npm run lint > lint-errors.txt 2>&1",
    "check": "npm run typecheck",
    "dev": "react-router dev",
    "export-routes": "npx react-router routes --json > routes.json",
    "lint": "eslint . --ignore-pattern 'temp/**'",
    "format": "npx prettier --write \"**/*.{ts,tsx,css}\"",
    "routes:json": "npx react-router routes --json",
    "routes": "npx react-router routes",
    "start": "react-router-serve ./build/server/index.js",
    "test:cloud": "gcloud run jobs --project studio-dev-e430 --region us-central1 execute studio-test --wait",
    "test:job": "npx playwright test --reporter=list",
    "test:report": "npx playwright test && npx playwright show-report",
    "test:ui": "npx playwright test --ui",
    "test": "npx playwright test",
    "typecheck:verbose": "react-router typegen && tsc --skipLibCheck --skipDefaultLibCheck --noEmit --excludeDirectories temp --pretty true > typecheck-errors.txt 2>&1",
    "typecheck": "react-router typegen && tsc --skipLibCheck --skipDefaultLibCheck --noEmit --excludeDirectories temp",
    "validate:verbose": "npm run check:verbose && npm run build > build-output.txt 2>&1",
    "validate": "npm run check && npm run build && npm run lint && npm run export-routes"
  },
